n            }\r\n        }\r\n\r\n        .dropdown-menu {\r\n            background-color: $white;\r\n            color: $grayCor;\r\n\r\n            @extend .fadeInDown;\r\n\r\n            a {\r\n                color: $grayCor;\r\n                text-align: center;\r\n                width: 100%;\r\n                height: 42px;\r\n                line-height: 42px;\r\n\r\n                &:hover {\r\n                    background-color: #f8f8f8;\r\n                }\r\n            }\r\n        }\r\n    }\r\n}\r\n\r\n.sidebar-toggle {\r\n    height: 30px;\r\n    line-height: 30px;\r\n    width: 100%;\r\n    text-align: center;\r\n    cursor: pointer;\r\n    background-color: lighten($sidebar-hover, 5%);\r\n\r\n    i {\r\n        color: $positive-txt;\r\n    }\r\n}\r\n\r\n.sidebar-btn-new {\r\n    margin: $baseGap auto;\r\n    width: 80%;\r\n\r\n    .ui-dropbox {\r\n        width: 100%;\r\n    }\r\n\r\n    .ui-btn {\r\n        @extend .ui-dropbox;\r\n        background-color: #585e6b;\r\n        border: 0;\r\n    }\r\n\r\n    .ui-drop-menu {\r\n        @extend .ui-dropbox;\r\n\r\n        li {\r\n            border-bottom: 1px solid #f0f0f0;\r\n            cursor: pointer;\r\n\r\n            &:last-child {\r\n                border-bottom: 0;\r\n            }\r\n\r\n            & > a {\r\n                color: #333;\r\n                padding-top: 8px;\r\n                padding-bottom: 8px;\r\n            }\r\n        }\r\n    }\r\n}\r\n\r\n.sidebar-menu {\r\n    width: $sidebar-width + 24px;\r\n    height: 100%;\r\n    overflow-x: hidden;\r\n    overflow-y: scroll;\r\n    padding-bottom: $header-height + 12px;\r\n\r\n    .ui-arrowadd:after {\r\n        right: 10px;\r\n        top: 12px;\r\n    }\r\n\r\n    li {\r\n        min-height: $sidebar-min-height;\r\n        line-height: $sidebar-min-height;\r\n\r\n        a {\r\n            display: block;\r\n            color: $positive-txt;\r\n\r\n            &.active {\r\n                color: $blue!important;\r\n\r\n                i {\r\n                    color: $blue;\r\n                    font-weight: normal;\r\n                }\r\n            }\r\n        }\r\n\r\n        i {\r\n            display: inline-block;\r\n            width: $sidebar-sm-width;\r\n            text-align: center;\r\n            font-size: $baseFontSize + 5;\r\n            color: $positive-ico;\r\n        }\r\n\r\n        &:hover {\r\n            background: $sidebar-hover;\r\n\r\n           &> a {\r\n                color: $white;\r\n            }\r\n        }\r\n    }\r\n\r\n    .sub-menu {\r\n        display: none;\r\n        text-align: left;\r\n\r\n        li {\r\n            padding-left: 18px;\r\n\r\n            a {\r\n                display: block;\r\n                color: $positive-txt;\r\n\r\n                &:hover {\r\n                    color: $white;\r\n                }\r\n            }\r\n        }\r\n    }\r\n\r\n    .open {\r\n        background: $sidebar-hover;\r\n        color: $white;\r\n\r\n        &:after {\r\n            content: \"\\e659\";\r\n        }\r\n    }\r\n\r\n    .open + ul {\r\n        background: $sidebar-hover;\r\n    }\r\n}\r\n\r\n.sidebar-close {\r\n    width: $sidebar-sm-width;\r\n    overflow: hidden;\r\n\r\n    .sidebar-toggle {\r\n        i {\r\n            &:before {\r\n                content: \"\\e668\";\r\n            }\r\n        }\r\n    }\r\n\r\n    .sub-menu {\r\n        li {\r\n            padding-left: 0;\r\n        }\r\n    }\r\n}\r\n\r\n.sidebar-close ~ .main-content {\r\n    margin-left: $sidebar-sm-width;\r\n}\r\n\r\n//侧边滑动编辑页\r\n#slide-eidt {\r\n    position: fixed;\r\n    min-width: $slide-min-width;\r\n    width: $slide-max-width;\r\n    height: 100%;\r\n    background-color: lighten($sidebar-bg, 14%);\r\n    z-index: 500;\r\n    overflow-y: scroll;\r\n    overflow-x: hidden;\r\n    margin-right: -20px;\r\n    top: 0;\r\n    right: - $slide-max-width;\r\n}\r\n\r\n.slide-cnt {\r\n    .scroll-wrapper {\r\n        margin-bottom: $fo
package com.maigangle.gyl.facade.entity.vo;

import java.math.BigDecimal;

import com.maigangle.gyl.facade.entity.BaseCompany;
import com.maigangle.gyl.facade.entity.BaseProcess;
import com.maigangle.gyl.facade.entity.BaseProduct;
import com.maigangle.gyl.facade.entity.ConContractDetail;
import com.maigangle.gyl.facade.entity.api.CrmClient;
import com.maigangle.gyl.facade.entity.api.DictMallMnfct;

public class ConContractDetailVo extends ConContractDetail {

	private static final long serialVersionUID = -3135942866543030142L;
	
	/**
	 * 卖方
	 */
	private BaseCompany seller;
	
	/**
	 * 买方
	 */
	private CrmClient buyer;
	
	/**
	 * 合同
	 */
	private ConContractVo contract;
	
	/**
	 * 品名
	 */
	private BaseProduct product;
	
	 /**
     * 表面
     */
	private BaseProcess process;
	
	/**
	 * 边部
	 */
	private String edgeName;

	/**
	 * 厂商
	 */
	private DictMallMnfct mnfct;
	/**
	 * 用于显示的重量
	 */
	private BigDecimal netWeightDisplay;


	public BaseCompany getSeller() {
		return seller;
	}

	public void setSeller(BaseCompany seller) {
		this.seller = seller;
	}

	public CrmClient getBuyer() {
		return buyer;
	}

	public void setBuyer(CrmClient buyer) {
		this.buyer = buyer;
	}

	public ConContractVo getContract() {
		return contract;
	}

	public void setContract(ConContractVo contract) {
		this.contract = contract;
	}

	public BaseProduct getProduct() {
		return product;
	}

	public void setProduct(BaseProduct product) {
		this.product = product;
	}

	public BaseProcess getProcess() {
		return process;
	}

	public void setProcess(BaseProcess process) {
		this.process = process;
	}

	public String getEdgeName() {
		return edgeName;
	}

	public void setEdgeName(String edgeName) {
		this.edgeName = edgeName;
	}

	public DictMallMnfct getMnfct() {
		return mnfct;
	}

	public void setMnfct(DictMallMnfct mnfct) {
		this.mnfct = mnfct;
	}

	public BigDecimal getNetWeightDisplay() {
		return netWeightDisplay;
	}

	public void setNetWeightDisplay(BigDecimal netWeightDisplay) {
		this.netWeightDisplay = netWeightDisplay;
	}
	
}
